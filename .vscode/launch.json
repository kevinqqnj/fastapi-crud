    "version": "0.2.0",
    "configurations": [
        {
            "name": "Python: unvicorn",
            "type": "python",
            "request": "launch",
            "module": "uvicorn",
            "args": [
                "app.main:app",
                "--host",
                "0.0.0.0",
                "--reload",
                "--log-level",
                "trace"
            ],
            "cwd": "${workspaceFolder}/src"
        },
        {
            "name": "DEBUG manage.py",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/src/manage.py",
            "console": "integratedTerminal",
            "cwd": "${workspaceFolder}/src"
        }
    ]
}